__package__ = ''

from pyconfig import Namespace

mongodb = Namespace()
mongodb.host = '<%= @mongohost %>'
mongodb.port = '<%= @mongoport %>'

strava = Namespace()
strava.client_id = <%= @stravaclientid %>
strava.client_secret = '<%= @stravaclientsecret %>'

cache = Namespace()
cache.data_dir = '/data/cache/data'
cache.lock_dir = '/data/cache/lock'

